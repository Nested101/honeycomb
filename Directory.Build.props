    <FakeItEasyVersion>7.1.0</FakeItEasyVersion>
    <FluentAssertionsVersion>6.0.0-beta0002</FluentAssertionsVersion>
    <MartinCostelloLoggingXUnitVersion>0.1.0</MartinCostelloLoggingXUnitVersion>
    <MicrosoftTestSdkVersion>16.10.0</MicrosoftTestSdkVersion>
    <MoqContribHttpClientVersion>1.3.0</MoqContribHttpClientVersion>
    <MoqVersion>4.16.1</MoqVersion>
    <OrleansTestKitVersion>3.1.6</OrleansTestKitVersion>
    <xUnitRunnerVisualStudio>2.4.3</xUnitRunnerVisualStudio>
    <xUnitVersion>2.4.1</xUnitVersion>
  </PropertyGroup>
